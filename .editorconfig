root = true

[*]
charset = utf-8
end_of_line = lf
insert_final_newline = true
trim_trailing_whitespace = true
indent_style = tab
indent_size = tab
tab_width = 8

[*.c]
tab_width = 4

[constants.h]
tab_width = 4
indent_style = space

[*.css]
tab_width = 2
indent_style = space

[configure]
tab_width = 4
